<table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
  <thead>
      <tr>
          <th>No</th>
          <th>Name</th>
          <th>Action</th>
      </tr>
  </thead>

  <tbody>
      <% for(let i = 0; i < resCategory.length; i++){ %>
        <tr>
            <td><%= i + 1 %></td>
            <td><%= resCategory[i].name %></td>
            <td>
                <a href="javascript:void()" type="button" class="btn btn-warning btn-circle btn-sm button-update" 
                    data-id="<%= resCategory[i].id %>"
                    data-name="<%= resCategory[i].name %>"
                >
                    <i class="fa fa-edit"></i>
                </a>

                <form action="/admin/category/<%= resCategory[i].id %>?_method=DELETE" method="POST" class="d-inline">
                    <button type="submit" class="btn btn-danger btn-circle btn-sm">
                        <i class="fa fa-trash"></i>
                    </button>
                </form>
            </td>
        </tr>
      <% } %>
  </tbody>
</table>