<div class="tab-pane fade mt-4" id="nav-profile" role="tabpanel" aria-labelledby="nav-profile-tab">
  <div class="card">
    <div class="card-body">
      <form method="POST" action="/admin/item">
        
        <div class="form-group">
          <label for="title">Title</label>
          <input type="text" name="title" class="form-control" id="title" placeholder="Example input">
        </div>
        
        <div class="form-group">
          <label for="price">price</label>
          <input type="number" name="proce" class="form-control" id="price" placeholder="Another input">
        </div>

        <div class="form-group">
          <label for="city">city</label>
          <input type="text" name="city" class="form-control" id="city" placeholder="Example input">
        </div>
        
        <div class="form-group">
          <label for="category">category</label>
          <select name="category" class="form-control" id="category">
            
            <% for(let i=1; i < resCategory.length; i++){ %>
              <option value="<%= resCategory[i].id %>"> <%= resCategory[i].name %> </option>
             <% } %>
            
          </select>
        </div>

        <div class="form-group">
          <label for="image">image</label>
          <input type="file" name="image" class="form-control" id="image" placeholder="Example input">
        </div>
        
        <div class="form-group">
          <label for="exampleFormControlTextarea1">description</label>
          <textarea name="about" class="form-control" id="exampleFormControlTextarea1" rows="3"></textarea>
        </div>

      </form>
    </div>
  </div>
</div>